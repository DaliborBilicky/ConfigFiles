  "tokenColors": [
    {
      "settings": {
        "foreground": "#ebdbb2"
      }
    },
    {
      "scope": [
        "meta.embedded",
        "source.groovy.embedded",
        "string meta.image.inline.markdown"
      ],
      "settings": {
        "foreground": "#ebdbb2"
      }
    },
    {
      "name": "Comments",
      "scope": ["comment", "string.quoted.docstring"],
      "settings": {
        "foreground": "#665c54",
        "fontStyle": "italic"
      }
    },
    {
      "name": "String",
      "scope": "string",
      "settings": {
        "foreground": "#b8bb26"
      }
    },
    {
      "name": "Template Definition",
      "scope": [
        "punctuation.definition.template-expression",
        "punctuation.section.embedded"
      ],
      "settings": {
        "foreground": "#fe8019"
      }
    },
    {
      "name": "Reset JavaScript string interpolation expression",
      "scope": ["meta.template.expression"],
      "settings": {
        "foreground": "#ebdbb2"
      }
    },
    {
      "name": "Number",
      "scope": "constant.numeric",
      "settings": {
        "foreground": "#d3869b"
      }
    },
    {
      "name": "Built-in constant",
      "scope": "constant.language",
      "settings": {
        "foreground": "#d3869b",
        "fontStyle": "italic"
      }
    },
    {
      "name": "User-defined constant",
      "scope": "constant.character, constant.other",
      "settings": {
        "foreground": "#d3869b"
      }
    },
    {
      "name": "Variable",
      "scope": "variable",
      "settings": {
        "fontStyle": "",
        "foreground": "#ebdbb2"
      }
    },
    {
      "name": "Keyword",
      "scope": "keyword",
      "settings": {
        "foreground": "#fe8019"
      }
    },
    {
      "name": "keyword operator",
      "scope": "keyword.operator, punctuation.separator.java",
      "settings": {
        "foreground": "#ebdbb2"
      }
    },
    {
      "name": "Storage",
      "scope": "storage",
      "settings": {
        "fontStyle": "",
        "foreground": "#fabd2f"
      }
    },
    {
      "name": "C++",
      "scope": "storage.modifier.reference.cpp, storage.modifier.pointer.cpp, punctuation.separator.pointer-access.cpp",
      "settings": {
        "foreground": "#ebdbb2"
      }
    },
    {
      "name": "C++2",
      "scope": "keyword.operator.delete.cpp, keyword.operator.new.cpp, keyword.operator.delete.array.cpp, keyword.operator.new.array.cpp",
